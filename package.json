    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/curran/chiasm.git"
  },
  "keywords": [
    "data",
    "visualization"
  ],
  "author": "Curran Kelleher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curran/chiasm/issues"
  },
  "homepage": "https://github.com/curran/chiasm",
  "dependencies": {
    "chai": "^3.2.0",
    "lodash": "^3.10.1",
    "model-js": "^0.2.4"
  },
  "devDependencies": {
    "browserify-shim": "^3.8.10",
    "browserify-versionify": "^1.0.6",
    "mocha": "^2.3.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-versionify"
    ]
