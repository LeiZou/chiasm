{
  "layout": {
    "plugin": "layout",
    "state": {
      "layout": {
        "orientation": "horizontal",
        "children": [
          "editor",
          {
            "orientation": "vertical",
            "children": [
              "barChart",
              "lineChart"
            ]
          }
        ]
      }
    }
  },
  "editor": {
    "plugin": "configEditor",
    "state": {
      "size": "400px"
    }
  },
  "barChart": {
    "plugin": "barChart",
    "state": {
      "xColumn": "letter",
      "xAxisLabel": "Letter",
      "yColumn": "frequency",
      "yAxisLabel": "Frequency",
      "sortColumn": "frequency",
      "sortOrder": "descending",
      "margin": {
        "top": 10,
        "right": 10,
        "bottom": 60,
        "left": 70
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "colorDefault": "#000000"
    }
  },
  "lineChart": {
    "plugin": "lineChart",
    "state": {
      "xColumn": "timestamp",
      "xAxisLabel": "Time",
      "yColumn": "temperature",
      "yAxisLabel": "Temperature",
      "margin": {
        "top": 10,
        "right": 10,
        "bottom": 60,
        "left": 70
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "colorDefault": "#000000"
    }
  },
  "barChartCSVLoader": {
    "plugin": "csvLoader",
    "state": {
      "csvPath": "testData/simpleBars.csv",
      "numericColumns": ["frequency"]
    }
  },
  "lineChartCSVLoader": {
    "plugin": "csvLoader",
    "state": {
      "csvPath": "testData/simpleTime.csv",
      "numericColumns": ["temperature"],
      "timeColumns": ["timestamp"]
    }
  },
  "links": {
    "plugin": "links",
    "state": {
      "bindings": [
        "barChartCSVLoader.data -> barChart.data",
        "lineChartCSVLoader.data -> lineChart.data"
      ]
    }
  }
}
