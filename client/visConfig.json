{
  "layout": {
    "plugin": "layout",
    "state": {
      "layout": {
        "orientation": "horizontal",
        "children": [
          "editor",
          {
            "orientation": "vertical",
            "children": [
              "A",
              {
                "orientation": "horizontal",
                "children": [
                  "B",
                  "C"
                ]
              }
            ]
          }
        ]
      }
    }
  },
  "editor": {
    "plugin": "configEditor",
    "state": {
      "size": "400px"
    }
  },
  "A": {
    "plugin": "barChart",
    "state": {
      "xColumn": "letter",
      "xAxisLabel": "Letter",
      "yColumn": "frequency",
      "yAxisLabel": "Frequency",
      "sortColumn": "frequency",
      "sortOrder": "descending",
      "margin": {
        "top": 10,
        "right": 10,
        "bottom": 60,
        "left": 70
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "colorDefault": "#000000"
    }
  },
  "B": {
    "plugin": "dummyVis",
    "state": {
      "text": "B",
      "color": "#a8f0ff",
      "size": 2
    }
  },
  "C": {
    "plugin": "dummyVis",
    "state": {
      "text": "C",
      "color": "#ffe2a8"
    }
  },
  "dataLoader": {
    "plugin": "csvLoader",
    "state": {
      "csvPath": "testData/simpleBars.csv",
      "numericColumns": ["frequency"]
    }
  },
  "links": {
    "plugin": "links",
    "state": {
      "bindings": [
        "dataLoader.data -> A.data"
      ]
    }
  }
}
